print("Star Wars Name Generator")
print()
first_name = input("Input your first name > ").capitalize().strip()
print()
last_name = input("Input your last name > ")
print()
maiden_name = input("Input your mother's maiden name> ").capitalize().strip()
print()
city = input("Input the city where you were born > ").capitalize().strip()
print()
print(f"Your Star Wars name is {first_name[:3]}{last_name[:3]} {maiden_name[:2]}{city[2:]}")
